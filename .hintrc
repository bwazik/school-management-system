{
  "extends": [
    "development"
  ],
  "hints": {
    "compat-api/css": [
      "default",
      {
        "ignore": [
          "-webkit-text-size-adjust",
          "text-align: -webkit-match-parent"
        ]
      }
    ],
    "css-prefix-order": "off"
  },
  "browserslist": [
    "defaults",
    "not ie 11",
    "not and_chr <= 127",
    "not chrome <= 127",
    "not edge <= 127",
    "not opera <= 111",
    "not samsung <= 25",
    "not and_ff <= 127",
    "not firefox <= 128",
    "not ios_saf <= 17.5",
    "not safari <= 17.5"
  ]
}